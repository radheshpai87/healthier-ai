{
  "expo": {
    "name": "AuraHealth",
    "slug": "aurahealth",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "extra": {
      "geminiApiKey": "AIzaSyCy1dFMLzFARAD_oWs04pq1zecl63AtR34",
      "backendUrl": "http://10.1.161.140:3000/api"
    },
    "splash": {
      "backgroundColor": "#FFF5F5"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "AuraHealth needs your location for emergency alerts to help responders find you."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFF5F5"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "SEND_SMS"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "aurahealth",
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-asset",
      "expo-font",
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "AuraHealth needs your location for emergency alerts."
        }
      ]
    ]
  }
}
